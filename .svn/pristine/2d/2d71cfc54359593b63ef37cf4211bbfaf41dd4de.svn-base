<meta name="keywords" content="关键词,5个左右,单个8汉字以内">
<meta name="description" content="网站描述，字数尽量空制在80个汉字，160个字符以内！">
</head>
<body ontouchstart>
 
<div class="containBox"> 
	 
	<div class="wap-container">
		 
		<nav class="breadcrumb">
			<div class="container">
				<i class="Hui-iconfont">&#xe67f;</i>
				<a href="/" class="c-primary">首页</a>
				<span class="c-gray en">&gt;</span>
				<a href="#">组件</a>
				<span class="c-gray en">&gt;</span>
				<span class="c-gray">当前页面</span>
			</div>
		</nav>
		<div class="container ui-sortable">
			<h3>用户等级添加</h3> 
			<div class="panel panel-default">
				<div class="panel-header clearfix">
				
				</div>
				<div class="panel-body">
					<form action="" method="post" class="form form-horizontal responsive" id="form-admin-add">
      	<table class="table table-border table-bordered table-bg">
        	<tbody>
          <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 用户名：</th>
            <td>		
		     <div class="formControls col-xs-8">
			<input id="adminName" type="text" class="input-text" placeholder="4~16个字符，字母/中文/数字/下划线"  name="adminName">
	        </div>
			</td>
          </tr>
          <tr>
          <th class="text-r"><span class="c-red">*</span> 所属等级：</th>
           <td>
		<span class="select-box">
       <select class="select" name="demo1">
        <option value="" selected>默认等级</option>
        <option value="1">等级一</option>
        <option value="2">等级二</option>
        <option value="3">等级三</option>
      </select>
    </span></td>
          </tr>       
          <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 邮箱：</th>
            <td>
			<div class="formControls col-xs-8">
		<input type="text"  class="input-text" placeholder="@" name="email" id="email" autocomplete="off"></div>
          </tr>
		    <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 手机号：</th>
            <td>
			<div class="formControls col-xs-8">
			<input type="text" class="input-text" autocomplete="off" placeholder="手机" name="phone	" id="phone">
			</div>
			</td>
          </tr>
		   <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 密码：</th>
            <td>
			<div class="formControls col-xs-8">
		<input type="password" class="input-text" autocomplete="off" placeholder="密码" name="password" id="password">
			</div>
			</td>
          </tr>
		   <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 密码验证：</th>
            <td>
		<div class="formControls col-xs-8">
		<input type="password" class="input-text" autocomplete="off" placeholder="密码"  name="password2" id="password2">
			</div>
			</td>
          </tr>
         
		 <tr>
            <th width="100" class="text-r"><span class="c-red">*</span> 性别：</th>
            <td>
			<div class="formControls skin-minimal col-xs-5">
			<div class="radio-box">
			<input type="radio" id="sex" name="sex">
			<label for="sex-1">男</label>
			</div>
			<div class="radio-box">
			<input type="radio" id="sex" name="sex">
			<label for="sex-2">女</label>
				</div>
			</div>
			</td>
          </tr>
          <tr>
            <th></th>
            <td>
			<button class="btn btn-success radius" type="submit">
			<i class="icon-ok"></i> 确定</button>
			</td>
          </tr>
        </tbody>
      </table>
    </form>
				</div>
			</div>
			       
			 
		</div>
		<footer class="footer mt-20">
			<div class="container">
				<nav class="footer-nav">
